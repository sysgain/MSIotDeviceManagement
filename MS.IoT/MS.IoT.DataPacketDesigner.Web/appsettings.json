{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "ClientId": "<clientID>",
    "ClientSecret": "<clientsecret>",
    "TenantId": "<tenantId>",
    "CallbackPath": "/signin-oidc",
    "GraphEndpoint": "https://graph.windows.net"
  },
  "IotHub": {
    "HostName": "<iothubname>.azure-devices.net",
    "ConnectionString": "HostName=<iothubname>.azure-devices.net;SharedAccessKeyName=iothubowner;SharedAccessKey=<key>"
  },
  "Blob": {
    "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=<storageaccountname>;AccountKey=<key>;EndpointSuffix=core.windows.net",
    "SimulateToolsContainerName": "simulate-tools"
  },
  "CosmosDb": {
    "Endpoint": "https://<cosmosdbname>.documents.azure.com:443/",
    "AuthKey": "<key>",
    "Database": "MSIoT",
    "Collections": {
      "Templates": "Templates",
      "Messages": "Messages",
      "Groups": "Groups"
    },
    "TemplatesBase": "https://msiotsolutiondev.blob.core.windows.net/cosmosdbtemplates/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
